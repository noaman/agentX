{
  "mcpServers": {
    "agentx": {
      "command": "uv",
      "args": [
        "--directory",
        "mcp",
        "run",
        "server.py"
      ]
    },
      "brave-search": {
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-brave-search"],
        "env": {
        "BRAVE_API_KEY": "BSA3Azo9avarAg8ipgK-9-VMSk4klsg"
      },
      "requires_confirmation": ["brave_web_search"]
    },  
    "ddg-search": {
            "command": "uvx",
            "args": ["duckduckgo-mcp-server"]
        },
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ]
    },
    "puppeteer": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-puppeteer"]
    },
    "websearch": {
        "command": "npx",
        "args": [
            "websearch-mcp"
        ],
        "environment": {
            "API_URL": "http://localhost:3001",
            "MAX_SEARCH_RESULT": "5" 
        }
    },
    "ElevenLabs": {
      "command": "uvx",
      "args": ["elevenlabs-mcp"],
      "env": {
        "ELEVENLABS_API_KEY": "<insert-your-api-key-here>"
      }
    },
   "simple-arxiv": {
          "command": "python3",
          "args": ["-m", "mcp_simple_arxiv"]
      },
    "youtube-transcript": {
      "command": "npx",
      "args": ["-y", "@kimtaeyoon83/mcp-server-youtube-transcript"]
    }
  
  

        
      
  
}
}